# Application Configuration
# Copy this file to .env and customize as needed

# The marketing site URL (this site)
VITE_SITE_URL=https://quaivault.org

# The URL of the Quai Vault web application
VITE_APP_URL=https://testnet.quaivault.org

# GitHub repository in the format "owner/repo"
VITE_GITHUB_REPO=Quai-Vault/quai-multisig-www

# Supabase Configuration (for live stats)
# These are optional - if not configured, static placeholder stats will be shown
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key
VITE_SUPABASE_SCHEMA=testnet
